Object subclass: #AlgoFix2	instanceVariableNames: 'pinturas pintores presupuestos'	classVariableNames: ''	package: 'TP1-Model2'!!AlgoFix2 methodsFor: 'initialization' stamp: 'Fer 5/21/2020 21:22'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !!AlgoFix2 methodsFor: 'registering' stamp: 'Fer 5/21/2020 20:49'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor|	pintor:=	Pintor2 new.	pintor setNombre: unNombrePintorPincel. 	pintor setValorHora: unValorHora.	pintor setTiempoPintarM2: 2.	pintores at: unNombrePintorPincel put: pintor.	! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:49'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor|	pintor:=	Pintor2 new.	pintor setNombre: unNombrePintorRodillo. 	pintor setValorHora: unValorHora.	pintor setTiempoPintarM2: 1.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:50'!getPintores^pintores.! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:40'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura "	^presupuestos elegirPresupuestoMasBarato."	| presu|		presupuestos:= pintores collect: [ :pintor |  			presu := Presupuesto2 new.		presu guardarElPintor: pintor conValorMinimo: (pintor calcularCostos: metros2 conPintura: unaInstanciaPintura).].	^ presu.	! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/20/2020 19:32'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 00:04'!getPinturas^pinturas.! !!AlgoFix2 methodsFor: 'creating' stamp: 'Fer 5/21/2020 20:55'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura pincel rodillo|	pintura := Pintura2 crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro .		pincel := Pincel inicializarPincelConManos: manosPincel yunTiempoM2: 1  yUnConsumo: 4.	pintura setPincel: pincel.		rodillo := Rodillo inicializaRodilloConManos: manosRodillo yunTiempoM2: 2 yUnConsumo: 5.	pintura setRodillo: rodillo.		pinturas at: unNombrePintura put: pintura.	^ pintura.	! !Object subclass: #Pincel	instanceVariableNames: 'manos tiempo consumo'	classVariableNames: ''	package: 'TP1-Model2'!!Pincel methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:03'!calcularMateriales: unCosto	^ unCosto *manos * tiempo.! !!Pincel methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:16'!calcularManoDeObra: metros2PorValorHora	^ metros2PorValorHora * manos * tiempo.! !!Pincel methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:56'!setConsumo: unconsumoconsumo := unconsumo .! !!Pincel methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:42'!setManos: cantidadManos	manos:= cantidadManos.! !!Pincel methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:53'!setTiempo: untiempotiempo:= untiempo .! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel class	instanceVariableNames: ''!!Pincel class methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:53'!inicializarPincelConManos: manosPincel yunTiempoM2: unTiempo	|pincel|	pincel := self new.	pincel setManos: manosPincel.	pincel setTiempo: unTiempo.	^pincel.! !!Pincel class methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:55'!inicializarPincelConManos: manosPincel yunTiempoM2: unTiempo yUnConsumo: unconsumo	|pincel|	pincel := self new.	pincel setManos: manosPincel.	pincel setTiempo: unTiempo.	pincel setConsumo: unconsumo.	^pincel.! !Object subclass: #Pintor2	instanceVariableNames: 'nombre valorHora tiempoM2'	classVariableNames: ''	package: 'TP1-Model2'!!Pintor2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:59'!initilize	nombre := String.	valorHora:= Integer.	! !!Pintor2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:36'!calcularCostos: metros2 conPintura: unaInstanciaPintura 	|materiales manoDeObra |	materiales := unaInstanciaPintura calcularMateriales: metros2.	manoDeObra := unaInstanciaPintura  calcularManoDeObra:  (metros2 * valorHora).	^ materiales+ manoDeObra.	! !!Pintor2 methodsFor: 'initialization' stamp: 'Fer 5/20/2020 23:47'!setTiempoPintarM2: unTiempo	tiempoM2 := unTiempo. ! !!Pintor2 methodsFor: 'initialization' stamp: 'Fer 5/20/2020 16:45'!setNombre: unNombre	nombre:=unNombre .! !!Pintor2 methodsFor: 'initialization' stamp: 'Fer 5/20/2020 23:47'!setValorHora: unValorHora 	valorHora := unValorHora.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor2 class	instanceVariableNames: 'tecnica'!Object subclass: #Pintura2	instanceVariableNames: 'nombrePintura precioLitro pincel rodillo'	classVariableNames: ''	package: 'TP1-Model2'!!Pintura2 methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:47'!setPincel: instanciaPincelpincel := instanciaPincel.! !!Pintura2 methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:47'!setRodillo: instanciaRodillo	rodillo := instanciaRodillo.! !!Pintura2 methodsFor: 'initialization' stamp: 'Fer 5/20/2020 16:46'!setNombre: unNombrePintura nombrePintura:=	unNombrePintura.	! !!Pintura2 methodsFor: 'initialization' stamp: 'Fer 5/20/2020 16:46'!setPrecioLitro: unPrecioLitroprecioLitro:= 	unPrecioLitro. 	! !!Pintura2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:05'!calcularMateriales: metros2	^ pincel calcularMateriales: (metros2 * precioLitro).! !!Pintura2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:16'!calcularManoDeObra: metros2PorValorHora"	manoDeObra:= metros2 * tiempoPintarM2  * valorHora  * (unaInstanciaPintura getManosPincel). "	^ pincel calcularManoDeObra: metros2PorValorHora .! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura2 class	instanceVariableNames: ''!!Pintura2 class methodsFor: 'creating' stamp: 'Fer 5/21/2020 20:45'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro 	|pintura |	pintura:= Pintura2 new.	pintura setNombre: unNombrePintura.	pintura setPrecioLitro: unPrecioLitro.	^pintura.! !!Pintura2 class methodsFor: 'creating' stamp: 'Fer 5/21/2020 00:10'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura tecnicaPincel tecnicaRodillo|	pintura:= Pintura2 new.	pintura setNombre: unNombrePintura.	pintura setPrecioLitro: unPrecioLitro.	tecnicaPincel := TecnicaDePintado new.	tecnicaPincel setManos: manosPincel.	tecnicaPincel setConsumoM2: 4.	tecnicaPincel setTiempoPintarM2: 2.	tecnicaRodillo := TecnicaDePintado new.	tecnicaRodillo setManos: manosRodillo.	tecnicaRodillo setConsumoM2: 5.	tecnicaRodillo setTiempoPintarM2: 1.		pintura guardarTecnicaPincel: tecnicaPincel. 	pintura guardarTecnicaRodillo: tecnicaRodillo.	^pintura.! !Object subclass: #Presupuesto2	instanceVariableNames: 'nombre valor'	classVariableNames: ''	package: 'TP1-Model2'!!Presupuesto2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:58'!elegirPresupuestoMasBarato	|minValor pintorMinPresupuesto instanciaPresupuesto| 	minValor := (presupuestos values) min.	pintorMinPresupuesto := (presupuestos keyAtValue: minValor) .	instanciaPresupuesto := Presupuesto new.	instanciaPresupuesto guardarPintor: pintorMinPresupuesto conValorMinimo: minValor.	^instanciaPresupuesto.! !!Presupuesto2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/20/2020 16:46'!valor	^ valor.! !!Presupuesto2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/20/2020 16:46'!responsable 	^ nombre.! !!Presupuesto2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:26'!guardarElPintor: pintorMinPresupuesto conValorMinimo: minimoPresupuesto	nombre:= pintorMinPresupuesto. 	valor := minimoPresupuesto.	! !Object subclass: #Rodillo	instanceVariableNames: 'manos tiempo consumo'	classVariableNames: ''	package: 'TP1-Model2'!!Rodillo methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:21'!calcularMateriales: unCosto	^ unCosto *manos * tiempo.! !!Rodillo methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 21:21'!calcularManoDeObra: metros2PorValorHora	^ metros2PorValorHora * manos * tiempo.! !!Rodillo methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:57'!setConsumo: unconsumoconsumo:= unconsumo.! !!Rodillo methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:42'!setManos: cantidadManos	manos:= cantidadManos.! !!Rodillo methodsFor: 'initialization' stamp: 'Fer 5/21/2020 20:53'!setTiempo: untiempotiempo:= untiempo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rodillo class	instanceVariableNames: ''!!Rodillo class methodsFor: 'as yet unclassified' stamp: 'Fer 5/21/2020 20:56'!inicializaRodilloConManos: cantidadRodillos yunTiempoM2: untiempo yUnConsumo: unconsumo |rodillo|rodillo := self new.rodillo setManos: cantidadRodillos .rodillo setTiempo: untiempo.rodillo setConsumo: unconsumo .^ rodillo.! !
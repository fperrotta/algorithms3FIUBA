Object subclass: #AlgoFix3	instanceVariableNames: 'pinturas pintores'	classVariableNames: ''	package: 'TP1-Model3'!!AlgoFix3 methodsFor: 'initialization' stamp: 'Fer 5/22/2020 17:35'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/26/2020 17:48'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor rodillo|	rodillo :=	 Rodillo3 inicializarRodilloConTiempoPintarUnM2: 1 yUnConsumo: 5.	pintor :=	Pintor3 inicializarPintorConNombre:  unNombrePintorRodillo conValorHora: unValorHora yHerramientaDeTrabajo: rodillo.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/26/2020 17:48'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura 	^ Presupuesto3 crearPresupuestosDe: pintores conPintura: unaInstanciaPintura yconMetros: metros2.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:35'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix3 methodsFor: 'registering' stamp: 'Fer 5/25/2020 18:11'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor pincel|	pincel :=	 Pincel3 inicializarPincelConTiempoPintarUnM2: 2 yUnConsumo: 4.	pintor := Pintor3 inicializarPintorConNombre: unNombrePintorPincel conValorHora: unValorHora yHerramientaDeTrabajo: pincel.	pintores at: unNombrePintorPincel put: pintor.	! !!AlgoFix3 methodsFor: 'creating' stamp: 'Fer 5/24/2020 15:33'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura|	pintura := Pintura3 crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo.	pinturas at: unNombrePintura put: pintura.	^ pintura.	! !Object subclass: #Pintor3	instanceVariableNames: 'nombre valorHora herramienta'	classVariableNames: ''	package: 'TP1-Model3'!!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:43'!calcularMaterial: metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularMaterial: metros2 conPintura: unaInstanciaPintura.! !!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:45'!calcularCostos: metros2 conPintura: unaInstanciaPintura 	|material manoDeObra|	material :=  self calcularMaterial: metros2 conPintura: unaInstanciaPintura .	manoDeObra := self calcularManoDeObra:metros2 conPintura: unaInstanciaPintura .	^ manoDeObra + material.! !!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:41'!calcularManoDeObra:metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: (valorHora * metros2).	! !!Pintor3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:31'!initilize	nombre := String.	valorHora:= Integer.	herramienta := ''.	! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombre	nombre:= unNombre.! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!herramienta: unaHerramienta	herramienta:= unaHerramienta.! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!valorHora: unValorHora 	valorHora:= unValorHora.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor3 class	instanceVariableNames: ''!!Pintor3 class methodsFor: 'initialize' stamp: 'Fer 5/25/2020 18:14'!inicializarPintorConNombre:  unNombrePintor conValorHora: unValorHora yHerramientaDeTrabajo: unaHerramienta 	|pintor|	pintor:=	Pintor3 new.	pintor nombre: unNombrePintor. 	pintor valorHora: unValorHora.	pintor herramienta: unaHerramienta.	^pintor.! !Object subclass: #Pintura3	instanceVariableNames: 'nombrePintura precioLitro manosPincel manosRodillo'	classVariableNames: ''	package: 'TP1-Model3'!!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombrePintura	nombrePintura:= unNombrePintura.	! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosPincel: unasManosPincel	manosPincel:= unasManosPincel.! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosRodillo: unasManosRodillo	manosRodillo:= unasManosRodillo.! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!precioLitro: unPrecioLitro	precioLitro:= 	unPrecioLitro. 	! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterialPincel: unCosto	^ precioLitro * manosPincel * unCosto.! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:44'!calcularManoDeObraPincel: unTiempo	^ manosPincel * unTiempo.! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterialRodillo: unCosto	^ precioLitro * manosRodillo * unCosto.! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:46'!calcularManoDeObraRodillo: unTiempo	^ manosRodillo * unTiempo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura3 class	instanceVariableNames: ''!!Pintura3 class methodsFor: 'initalize' stamp: 'Fer 5/25/2020 18:19'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura |	pintura:= Pintura3 new.	pintura nombre: unNombrePintura.	pintura precioLitro: unPrecioLitro.	pintura manosPincel: manosPincel.	pintura manosRodillo: manosRodillo.	^ pintura.! !Object subclass: #Presupuesto3	instanceVariableNames: 'pintores pintura presupuestos'	classVariableNames: ''	package: 'TP1-Model3'!!Presupuesto3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintura: unaPintura	pintura := unaPintura.! !!Presupuesto3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintores: unosPintores	pintores := unosPintores.! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/25/2020 18:11'!elegirPresupuestoMasBarato	^ PresupuestoMasBarato3 dePintor: (self responsable) conValor: (self valor).! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:42'!responsable 	^ presupuestos keyAtValue: ((presupuestos values) min).! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:58'!generarPresupuestos: metros2	presupuestos := pintores collect: [ :pintor |  			pintor calcularCostos: metros2 conPintura: pintura. ].	! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:42'!valor	^ (presupuestos values) min.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto3 class	instanceVariableNames: ''!!Presupuesto3 class methodsFor: 'initalize' stamp: 'Fer 5/25/2020 18:22'!crearPresupuestosDe: unosPintores conPintura: unaInstanciaPintura yconMetros: metros2	|presupuestos p|		presupuestos := self new.	presupuestos pintores: unosPintores.	presupuestos pintura: unaInstanciaPintura.	presupuestos generarPresupuestos: metros2.  	p := presupuestos elegirPresupuestoMasBarato.	^ p.! !Object subclass: #PresupuestoMasBarato3	instanceVariableNames: 'valorMinimo pintor'	classVariableNames: ''	package: 'TP1-Model3'!!PresupuestoMasBarato3 methodsFor: 'accesing' stamp: 'Fer 5/24/2020 17:16'!responsable 	^ pintor.! !!PresupuestoMasBarato3 methodsFor: 'accesing' stamp: 'Fer 5/24/2020 17:21'!valor	^ valorMinimo.! !!PresupuestoMasBarato3 methodsFor: 'initialization - private' stamp: 'Fer 5/24/2020 17:59'!guardarMenorPresupuestoDe: unPintor conValor: unValor	pintor := unPintor.	valorMinimo := unValor.! !!PresupuestoMasBarato3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:22'!initialize	pintor := String.	valorMinimo := Integer.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PresupuestoMasBarato3 class	instanceVariableNames: ''!!PresupuestoMasBarato3 class methodsFor: 'initalize' stamp: 'Fer 5/24/2020 17:59'!dePintor: unResponsable conValor: unValor	|presupuestosMasBarato|	presupuestosMasBarato := self new.	presupuestosMasBarato guardarMenorPresupuestoDe: unResponsable conValor: unValor.	^ presupuestosMasBarato.! !Object subclass: #Tecnica3	instanceVariableNames: 'tiempo consumo'	classVariableNames: ''	package: 'TP1-Model3'!!Tecnica3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!tiempo: unTiempo	tiempo:= unTiempo .! !!Tecnica3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!consumo: unconsumo	consumo := unconsumo .! !!Tecnica3 methodsFor: 'abstract' stamp: 'Fer 5/24/2020 14:33'!calcularMaterial: metros2 conPintura: unaPintura	self subclassResponsibility .	! !!Tecnica3 methodsFor: 'abstract' stamp: 'Fer 5/26/2020 17:49'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2 yCostoPintor: unCostoPintor	self subclassResponsibility .! !Tecnica3 subclass: #Pincel3	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Pincel3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterial: metros2 conPintura: unaPintura	^ unaPintura calcularMaterialPincel: (metros2 * consumo).! !!Pincel3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:50'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor	(metros2 > 40) ifTrue: 		[^ Pincel3ConDescuento calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 conTiempo:tiempo conConsumo:consumo yCostoPintor: unCostoPintor.]		ifFalse:		[^ unaInstanciaPintura calcularManoDeObraPincel: (tiempo * unCostoPintor).].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel3 class	instanceVariableNames: ''!!Pincel3 class methodsFor: 'initalize' stamp: 'Fer 5/25/2020 18:26'!inicializarPincelConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|pincel|	pincel := self new.	pincel tiempo: unTiempo.	pincel consumo: unconsumo.	^pincel.! !Pincel3 subclass: #Pincel3ConDescuento	instanceVariableNames: 'descuento'	classVariableNames: ''	package: 'TP1-Model3'!!Pincel3ConDescuento methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:51'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2 yCostoPintor: unCostoPintor	^ unaInstanciaPintura calcularManoDeObraPincel: (tiempo * unCostoPintor * descuento).! !!Pincel3ConDescuento methodsFor: 'accessing' stamp: 'Fer 5/26/2020 17:52'!descuento: unDescuento	descuento:= unDescuento.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel3ConDescuento class	instanceVariableNames: ''!!Pincel3ConDescuento class methodsFor: 'initalize' stamp: 'Fer 5/26/2020 17:52'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 conTiempo: unTiempo conConsumo: unConsumo yCostoPintor: unCostoPintor	|pincel |	pincel := self new.	pincel tiempo: unTiempo.	pincel consumo: unConsumo.	pincel descuento: 0.50 .	^ pincel calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor .! !Tecnica3 subclass: #Rodillo3	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Rodillo3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterial: metros2 conPintura: unaPintura	|material|	material := metros2 * consumo.	^ unaPintura calcularMaterialRodillo: material. 		! !!Rodillo3 methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:46'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor 	^ unaInstanciaPintura calcularManoDeObraRodillo: (tiempo*unCostoPintor).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rodillo3 class	instanceVariableNames: ''!!Rodillo3 class methodsFor: 'initialize' stamp: 'Fer 5/25/2020 18:26'!inicializarRodilloConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|rodillo|	rodillo := self new.	rodillo tiempo: unTiempo.	rodillo consumo: unconsumo.	^rodillo.! !
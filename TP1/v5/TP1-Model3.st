Object subclass: #AlgoFix3	instanceVariableNames: 'pinturas pintores'	classVariableNames: ''	package: 'TP1-Model3'!!AlgoFix3 methodsFor: 'initialization' stamp: 'Fer 5/22/2020 17:35'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:36'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor rodillo|	rodillo:=	 Rodillo2 inicializarRodilloConTiempoPintarUnM2:  1 yUnConsumo: 5.	pintor:=	Pintor3 inicializarPintorConNombre:  unNombrePintorRodillo conValorHora: unValorHora yHerramientaDeTrabajo: rodillo.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:53'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura 	|presupuesto|	presupuesto := Presupuesto3 crearPresupuestosDe: pintores conPintura: unaInstanciaPintura yconMetros: metros2.	^ presupuesto.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:35'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix3 methodsFor: 'registering' stamp: 'Fer 5/24/2020 17:36'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor pincel|	pincel :=	 Pincel2 inicializarPincelConTiempoPintarUnM2: 2 yUnConsumo: 4.	pintor := Pintor3 inicializarPintorConNombre: unNombrePintorPincel conValorHora: unValorHora yHerramientaDeTrabajo: pincel.	pintores at: unNombrePintorPincel put: pintor.	! !!AlgoFix3 methodsFor: 'creating' stamp: 'Fer 5/24/2020 15:33'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura|	pintura := Pintura3 crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo.	pinturas at: unNombrePintura put: pintura.	^ pintura.	! !Object subclass: #Pintor3	instanceVariableNames: 'nombre valorHora herramienta'	classVariableNames: ''	package: 'TP1-Model3'!!Pintor3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:27'!setValorHora: unValorHora 	valorHora:= unValorHora.! !!Pintor3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:27'!setHerramientaDeTrabajo: unaHerramienta	herramienta:= unaHerramienta.! !!Pintor3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:27'!setNombre: unNombre	nombre:= unNombre.! !!Pintor3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:43'!calcularMaterial: metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularMaterial: metros2 conPintura: unaInstanciaPintura.! !!Pintor3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:48'!presupuestarValorHora: metros2 	(metros2> 40) ifTrue: 		[^ valorHora * 0.50]		ifFalse:		[^ valorHora].! !!Pintor3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:54'!calcularManoDeObra:metros2 conPintura: unaInstanciaPintura 	|coberturaTiempo|	coberturaTiempo :=  herramienta calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2. 	^ coberturaTiempo * valorHora * metros2 .! !!Pintor3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:31'!initilize	nombre := String.	valorHora:= Integer.	herramienta := ''.	! !!Pintor3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:45'!calcularCostos: metros2 conPintura: unaInstanciaPintura 	|material manoDeObra|	material :=  self calcularMaterial: metros2 conPintura: unaInstanciaPintura .	manoDeObra := self calcularManoDeObra:metros2 conPintura: unaInstanciaPintura .	^ manoDeObra + material.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor3 class	instanceVariableNames: ''!!Pintor3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 15:26'!inicializarPintorConNombre:  unNombrePintor conValorHora: unValorHora yHerramientaDeTrabajo: unaHerramienta 	|pintor|	pintor:=	Pintor3 new.	pintor setNombre: unNombrePintor. 	pintor setValorHora: unValorHora.	pintor setHerramientaDeTrabajo: unaHerramienta.	^pintor.! !Object subclass: #Pintura3	instanceVariableNames: 'nombrePintura precioLitro manosPincel manosRodillo'	classVariableNames: ''	package: 'TP1-Model3'!!Pintura3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:16'!setPrecioLitro: unPrecioLitro	precioLitro:= 	unPrecioLitro. 	! !!Pintura3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:16'!setManosPincel: unasManosPincel	manosPincel:= unasManosPincel.! !!Pintura3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:17'!setNombre: unNombrePintura	nombrePintura:= unNombrePintura.	! !!Pintura3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:16'!setManosRodillo: unasManosRodillo	manosRodillo:= unasManosRodillo.! !!Pintura3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:21'!brindarRendimientoPincel	^ manosPincel.! !!Pintura3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:21'!brindarRendimientoRodillo	^ manosRodillo.! !!Pintura3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:19'!brindarCostosRodillo	^ precioLitro * manosRodillo.! !!Pintura3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:18'!brindarCostosPincel	^ precioLitro * manosPincel.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura3 class	instanceVariableNames: ''!!Pintura3 class methodsFor: 'creating' stamp: 'Fer 5/24/2020 15:17'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura |	pintura:= Pintura3 new.	pintura setNombre: unNombrePintura.	pintura setPrecioLitro: unPrecioLitro.	pintura setManosPincel: manosPincel.	pintura setManosRodillo: manosRodillo.	^ pintura.! !Object subclass: #Presupuesto3	instanceVariableNames: 'pintores pintura presupuestos'	classVariableNames: ''	package: 'TP1-Model3'!!Presupuesto3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:23'!elegirPresupuestoMasBarato	^ PresupuestoMasBarato masBarato: (self responsable) conValor: (self valor).! !!Presupuesto3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:12'!valor	^ 	(presupuestos values) min.! !!Presupuesto3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:34'!generarPresupuestos:metros2	presupuestos := pintores collect: [ :pintor |  			pintor calcularCostos: metros2 conPintura: pintura. ].	! !!Presupuesto3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:12'!responsable 	^ presupuestos keyAtValue: ((presupuestos values) min) .! !!Presupuesto3 methodsFor: 'initialization' stamp: 'Fer 5/22/2020 17:34'!setPintores: unosPintores	pintores := unosPintores.! !!Presupuesto3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:15'!setPintura: unaPintura	pintura := unaPintura.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto3 class	instanceVariableNames: ''!!Presupuesto3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:10'!crearPresupuestosDe: unosPintores conPintura: unaInstanciaPintura yconMetros: metros2	|presupuestos p|		presupuestos := self new.	presupuestos setPintores: unosPintores.	presupuestos setPintura: unaInstanciaPintura.	presupuestos generarPresupuestos: metros2.  	p := presupuestos elegirPresupuestoMasBarato.	^ p.! !Object subclass: #PresupuestoMasBarato	instanceVariableNames: 'valorMinimo pintor'	classVariableNames: ''	package: 'TP1-Model3'!!PresupuestoMasBarato methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:16'!responsable 	^ pintor.! !!PresupuestoMasBarato methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:21'!valor	^ valorMinimo.! !!PresupuestoMasBarato methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:22'!guardarMenorPresupuestoDe: unResponsable conValor: unValor	pintor := unResponsable.	valorMinimo := unValor.! !!PresupuestoMasBarato methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:22'!initialize	pintor := String.	valorMinimo := Integer.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PresupuestoMasBarato class	instanceVariableNames: ''!!PresupuestoMasBarato class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:21'!masBarato: unResponsable conValor: unValor	|presupuestosMasBarato|	presupuestosMasBarato := self new.	presupuestosMasBarato guardarMenorPresupuestoDe: unResponsable conValor: unValor.	^ presupuestosMasBarato.! !Object subclass: #Tecnica	instanceVariableNames: 'tiempo consumo'	classVariableNames: ''	package: 'TP1-Model3'!!Tecnica methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 14:33'!calcularMaterial: metros2 conPintura: unaPintura	self subclassResponsibility .	! !!Tecnica methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:50'!calcularManoDeObraConPintura: unaInstanciaPintura 	self subclassResponsibility .! !!Tecnica methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:36'!setTiempoPintarUnM2: unTiempo	tiempo:= unTiempo .! !!Tecnica methodsFor: 'initialization' stamp: 'Fer 5/24/2020 15:36'!setConsumo: unconsumo	consumo := unconsumo .! !Tecnica subclass: #Pincel2	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Pincel2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:53'!calcularMaterial: metros2 conPintura: unaPintura	|rendimiento material|	rendimiento := unaPintura brindarCostosPincel. 	material := metros2 * consumo * rendimiento. 	^ material.! !!Pincel2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:53'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2	^ unaInstanciaPintura brindarRendimientoPincel * tiempo * (self determinarDescuento: metros2).! !!Pincel2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:51'!determinarDescuento: metros2 	(metros2 > 40) ifTrue: 		[^ 0.50]		ifFalse:		[^ 1].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel2 class	instanceVariableNames: ''!!Pincel2 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 15:48'!inicializarPincelConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|pincel|	pincel := self new.	pincel setTiempoPintarUnM2: unTiempo.	pincel setConsumo: unconsumo.	^pincel.! !Tecnica subclass: #Rodillo2	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Rodillo2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:19'!calcularMaterial: metros2 conPintura: unaPintura	|rendimiento material|	rendimiento := unaPintura brindarCostosRodillo. 	material := metros2 * consumo * rendimiento.	^ material.! !!Rodillo2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:28'!calcularManoDeObraConPintura: unaInstanciaPintura 	^ unaInstanciaPintura brindarRendimientoRodillo * tiempo.! !!Rodillo2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:55'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2	^ unaInstanciaPintura brindarRendimientoRodillo * tiempo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rodillo2 class	instanceVariableNames: ''!!Rodillo2 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 16:34'!inicializarRodilloConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|rodillo|	rodillo := self new.	rodillo setTiempoPintarUnM2: unTiempo.	rodillo setConsumo: unconsumo.	^rodillo.! !
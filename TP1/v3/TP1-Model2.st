Object subclass: #AlgoFix2	instanceVariableNames: 'pinturas pintores'	classVariableNames: ''	package: 'TP1-Model2'!!AlgoFix2 methodsFor: 'registering' stamp: 'Fer 5/22/2020 16:55'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor|	pintor:=	Pintor2 inicializarPintorConNombre: unNombrePintorPincel conValorHora:unValorHora  yTiempoM2: 2 conTecnica:1.	pintores at: unNombrePintorPincel put: pintor.	! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 16:55'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor|	pintor:=	Pintor2 inicializarPintorConNombre: unNombrePintorRodillo conValorHora:  unValorHora  yTiempoM2: 1 conTecnica: 0.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 16:16'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura 	|presupuesto|	presupuesto := Presupuesto2 elegirPresupuestoMasBaratoDe: pintores conPintura: unaInstanciaPintura yconMetros: metros2.	^ presupuesto. ! !!AlgoFix2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/20/2020 19:32'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix2 methodsFor: 'creating' stamp: 'Fer 5/22/2020 16:33'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura pincel rodillo|	pintura := Pintura2 crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro .		pincel := Pincel inicializarPincelConManos: manosPincel yUnConsumo: 4.	pintura setPincel: pincel.		rodillo := Rodillo inicializaRodilloConManos: manosRodillo yUnConsumo: 5.	pintura setRodillo: rodillo.		pinturas at: unNombrePintura put: pintura.	^ pintura.	! !!AlgoFix2 methodsFor: 'initialization' stamp: 'Fer 5/21/2020 21:22'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !Object subclass: #P
ecioLitro: unPrecioLitroprecioLitro:= 	unPrecioLitro. 	! !!Pintura2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:22'!calcularMateriales: metros2 conTecnica: unaTecnica	(unaTecnica = 1 ) ifTrue:  [  		^ pincel calcularMateriales: (metros2 * precioLitro).]	 ifFalse: [		^ rodillo calcularMateriales: (metros2 * precioLitro)].! !!Pintura2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:12'!calcularManoDeObra: metros2PorValorHoraPorTiempo conTecnica: unaTecnica 	(unaTecnica = 1 ) ifTrue:  [  			^ pincel calcularManoDeObra: metros2PorValorHoraPorTiempo  ]		ifFalse: [			^ rodillo calcularManoDeObra: metros2PorValorHoraPorTiempo  ].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura2 class	instanceVariableNames: ''!!Pintura2 class methodsFor: 'creating' stamp: 'Fer 5/21/2020 20:45'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro 	|pintura |	pintura:= Pintura2 new.	pintura setNombre: unNombrePintura.	pintura setPrecioLitro: unPrecioLitro.	^pintura.! !!Pintura2 class methodsFor: 'creating' stamp: 'Fer 5/21/2020 00:10'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura tecnicaPincel tecnicaRodillo|	pintura:= Pintura2 new.	pintura setNombre: unNombrePintura.	pintura setPrecioLitro: unPrecioLitro.	tecnicaPincel := TecnicaDePintado new.	tecnicaPincel setManos: manosPincel.	tecnicaPincel setConsumoM2: 4.	tecnicaPincel setTiempoPintarM2: 2.	tecnicaRodillo := TecnicaDePintado new.	tecnicaRodillo setManos: manosRodillo.	tecnicaRodillo setConsumoM2: 5.	tecnicaRodillo setTiempoPintarM2: 1.		pintura guardarTecnicaPincel: tecnicaPincel. 	pintura guardarTecnicaRodillo: tecnicaRodillo.	^pintura.! !Object subclass: #Presupuesto2	instanceVariableNames: 'pintores pintura presupuestos'	classVariableNames: ''	package: 'TP1-Model2'!!Presupuesto2 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 15:52'
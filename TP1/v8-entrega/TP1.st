Object subclass: #AlgoFix	instanceVariableNames: 'pinturas pintores'	classVariableNames: ''	package: 'TP1-Model'!!AlgoFix methodsFor: 'initialization' stamp: 'Fer 5/22/2020 17:35'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !!AlgoFix methodsFor: 'private testing' stamp: 'Fer 5/27/2020 16:30'!pintores	^ pintores.! !!AlgoFix methodsFor: 'private testing' stamp: 'Fer 5/27/2020 16:30'!pinturas	^ pinturas.! !!AlgoFix methodsFor: 'calculations' stamp: 'Fer 5/28/2020 15:22'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura 	|presupuestos| 	presupuestos := Presupuestos crearConPintores: pintores conPintura: unaInstanciaPintura yconMetros: metros2.	^ PresupuestoMasBarato con: presupuestos .! !!AlgoFix methodsFor: 'calculations' stamp: 'Fer 5/22/2020 17:35'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix methodsFor: 'calculations' stamp: 'Fer 5/28/2020 15:23'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor rodillo|	rodillo := Rodillo crearConTiempoPintarUnM2: 1 yUnConsumo: 5.	pintor :=	Pintor crearConNombre:  unNombrePintorRodillo conValorHora: unValorHora yHerramientaDeTrabajo: rodillo.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix methodsFor: 'registering' stamp: 'Fer 5/28/2020 15:21'!crearConNombre: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura|	pintura := Pintura crearConNombre: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo.	pinturas at: unNombrePintura put: pintura.	^ pintura.	! !!AlgoFix methodsFor: 'registering' stamp: 'Fer 5/28/2020 15:22'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor pincel|	pincel :=	 Pincel crearConTiempoPintarUnM2: 2 yUnConsumo: 4.	pintor := Pintor crearConNombre: unNombrePintorPincel conValorHora: unValorHora yHerramientaDeTrabajo: pincel.	pintores at: unNombrePintorPincel put: pintor.	! !Object subclass: #Pintor	instanceVariableNames: 'nombre valorHora herramienta'	classVariableNames: ''	package: 'TP1-Model'!!Pintor methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:43'!calcularMaterial: metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularMaterial: metros2 conPintura: unaInstanciaPintura.! !!Pintor methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:41'!calcularManoDeObra:metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: (valorHora * metros2).	! !!Pintor methodsFor: 'calculations' stamp: 'Fer 5/27/2020 18:03'!calcularCostos: metros2 conPintura: unaInstanciaPintura 	|material manoDeObra|	material :=  self calcularMaterial: metros2 conPintura: unaInstanciaPintura .	manoDeObra := self calcularManoDeObra:metros2 conPintura: unaInstanciaPintura .	^ manoDeObra + material.! !!Pintor methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:31'!initilize	nombre := String.	valorHora:= Integer.	herramienta := ''.	! !!Pintor methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombre	nombre:= unNombre.! !!Pintor methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!herramienta: unaHerramienta	herramienta:= unaHerramienta.! !!Pintor methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!valorHora: unValorHora 	valorHora:= unValorHora.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor class	instanceVariableNames: ''!!Pintor class methodsFor: 'initialize' stamp: 'Fer 5/28/2020 15:21'!crearConNombre:  unNombrePintor conValorHora: unValorHora yHerramientaDeTrabajo: unaHerramienta 	|pintor|	pintor:=	Pintor new.	pintor nombre: unNombrePintor. 	pintor valorHora: unValorHora.	pintor herramienta: unaHerramienta.	^ pintor.! !Object subclass: #Pintura	instanceVariableNames: 'nombrePintura precioLitro manosPincel manosRodillo'	classVariableNames: ''	package: 'TP1-Model'!!Pintura methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombrePintura	nombrePintura:= unNombrePintura.	! !!Pintura methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosPincel: unasManosPincel	manosPincel:= unasManosPincel.! !!Pintura methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosRodillo: unasManosRodillo	manosRodillo:= unasManosRodillo.! !!Pintura methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!precioLitro: unPrecioLitro	precioLitro:= 	unPrecioLitro. 	! !!Pintura methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterialPincel: unCosto	^ precioLitro * manosPincel * unCosto.! !!Pintura methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:44'!calcularManoDeObraPincel: unTiempo	^ manosPincel * unTiempo.! !!Pintura methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterialRodillo: unCosto	^ precioLitro * manosRodillo * unCosto.! !!Pintura methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:46'!calcularManoDeObraRodillo: unTiempo	^ manosRodillo * unTiempo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura class	instanceVariableNames: ''!!Pintura class methodsFor: 'initalize' stamp: 'Fer 5/28/2020 15:21'!crearConNombre: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura |	pintura:= Pintura new.	pintura nombre: unNombrePintura.	pintura precioLitro: unPrecioLitro.	pintura manosPincel: manosPincel.	pintura manosRodillo: manosRodillo.	^ pintura.! !Object subclass: #PresupuestoMasBarato	instanceVariableNames: 'presupuestos'	classVariableNames: ''	package: 'TP1-Model'!!PresupuestoMasBarato methodsFor: 'calculations' stamp: 'Fer 5/27/2020 17:12'!responsable	^ presupuestos keyAtValue: ((presupuestos values) min).! !!PresupuestoMasBarato methodsFor: 'calculations' stamp: 'Fer 5/27/2020 17:12'!valor	^ (presupuestos values) min.! !!PresupuestoMasBarato methodsFor: 'initialization - private' stamp: 'Fer 5/27/2020 17:57'!presupuestos: unosPresupuestos	unosPresupuestos isEmpty ifTrue:[	PresupuestosVacioError signal]	ifFalse: [		presupuestos:= unosPresupuestos		.].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PresupuestoMasBarato class	instanceVariableNames: ''!!PresupuestoMasBarato class methodsFor: 'initalize' stamp: 'Fer 5/27/2020 17:11'!con: unosPresupuestos	|presupuestosMasBarato|	presupuestosMasBarato := self new.	presupuestosMasBarato presupuestos: unosPresupuestos.	^ presupuestosMasBarato.! !Object subclass: #Presupuestos	instanceVariableNames: 'pintores pintura presupuestos'	classVariableNames: ''	package: 'TP1-Model'!!Presupuestos methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintura: unaPintura	pintura := unaPintura.! !!Presupuestos methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintores: unosPintores	pintores := unosPintores.! !!Presupuestos methodsFor: 'calculations' stamp: 'Fer 5/27/2020 17:05'!generarPresupuestos: metros2	presupuestos := pintores collect: [ :pintor |  			pintor calcularCostos: metros2 conPintura: pintura. ].	^ presupuestos.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuestos class	instanceVariableNames: ''!!Presupuestos class methodsFor: 'initalize' stamp: 'Fer 5/28/2020 15:22'!crearConPintores: unosPintores conPintura: unaInstanciaPintura yconMetros: metros2	|presupuestos p|		presupuestos := self new.	presupuestos pintores: unosPintores.	presupuestos pintura: unaInstanciaPintura.	^ presupuestos generarPresupuestos: metros2.  	! !Error subclass: #PresupuestosVacioError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model'!Object subclass: #Tecnica	instanceVariableNames: 'tiempo consumo'	classVariableNames: ''	package: 'TP1-Model'!!Tecnica methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!tiempo: unTiempo	tiempo:= unTiempo .! !!Tecnica methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!consumo: unconsumo	consumo := unconsumo .! !!Tecnica methodsFor: 'abstract' stamp: 'Fer 5/24/2020 14:33'!calcularMaterial: metros2 conPintura: unaPintura	self subclassResponsibility .	! !!Tecnica methodsFor: 'abstract' stamp: 'Fer 5/26/2020 17:49'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2 yCostoPintor: unCostoPintor	self subclassResponsibility .! !Tecnica subclass: #Pincel	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model'!!Pincel methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterial: metros2 conPintura: unaPintura	^ unaPintura calcularMaterialPincel: (metros2 * consumo).! !!Pincel methodsFor: 'calculations' stamp: 'Fer 5/26/2020 19:30'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor	(metros2 > 40) ifTrue: 		[^ PincelConDescuento calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 conTiempo:tiempo conConsumo:consumo yCostoPintor: unCostoPintor.]		ifFalse:		[^ unaInstanciaPintura calcularManoDeObraPincel: (tiempo * unCostoPintor).].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel class	instanceVariableNames: ''!!Pincel class methodsFor: 'initalize' stamp: 'Fer 5/28/2020 15:22'!crearConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|pincel|	pincel := self new.	pincel tiempo: unTiempo.	pincel consumo: unconsumo.	^pincel.! !Pincel subclass: #PincelConDescuento	instanceVariableNames: 'descuento'	classVariableNames: ''	package: 'TP1-Model'!!PincelConDescuento methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:51'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2 yCostoPintor: unCostoPintor	^ unaInstanciaPintura calcularManoDeObraPincel: (tiempo * unCostoPintor * descuento).! !!PincelConDescuento methodsFor: 'accessing' stamp: 'Fer 5/26/2020 17:52'!descuento: unDescuento	descuento:= unDescuento.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PincelConDescuento class	instanceVariableNames: ''!!PincelConDescuento class methodsFor: 'initalize' stamp: 'Fer 5/27/2020 18:37'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 conTiempo: unTiempo conConsumo: unConsumo yCostoPintor: unCostoPintor	|pincel |	pincel := self new.	pincel tiempo: unTiempo.	pincel consumo: unConsumo.	pincel descuento: 0.50 .	^ pincel calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor.! !Tecnica subclass: #Rodillo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model'!!Rodillo methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:47'!calcularMaterial: metros2 conPintura: unaPintura	|material|	material := metros2 * consumo.	^ unaPintura calcularMaterialRodillo: material. 		! !!Rodillo methodsFor: 'calculations' stamp: 'Fer 5/26/2020 17:46'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2 yCostoPintor: unCostoPintor 	^ unaInstanciaPintura calcularManoDeObraRodillo: (tiempo*unCostoPintor).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rodillo class	instanceVariableNames: ''!!Rodillo class methodsFor: 'initialize' stamp: 'Fer 5/28/2020 15:23'!crearConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|rodillo|	rodillo := self new.	rodillo tiempo: unTiempo.	rodillo consumo: unconsumo.	^rodillo.! !
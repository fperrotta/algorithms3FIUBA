Object subclass: #AlgoFix3	instanceVariableNames: 'pinturas pintores'	classVariableNames: ''	package: 'TP1-Model3'!!AlgoFix3 methodsFor: 'initialization' stamp: 'Fer 5/22/2020 17:35'!initialize	pinturas := Dictionary new. 	pintores := Dictionary new.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:11'!registrarPintorDeRodillo: unNombrePintorRodillo conValorHora: unValorHora	|pintor rodillo|	rodillo:=	 Rodillo3 inicializarRodilloConTiempoPintarUnM2:  1 yUnConsumo: 5.	pintor:=	Pintor3 inicializarPintorConNombre:  unNombrePintorRodillo conValorHora: unValorHora yHerramientaDeTrabajo: rodillo.	pintores at: unNombrePintorRodillo put: pintor.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:07'!presupuestoMasBaratoParaPintarMetrosCuadrados: metros2 conPintura: unaInstanciaPintura 	|presupuesto|	presupuesto := Presupuesto3 crearPresupuestosDe: pintores conPintura: unaInstanciaPintura yconMetros: metros2.	^ presupuesto.! !!AlgoFix3 methodsFor: 'as yet unclassified' stamp: 'Fer 5/22/2020 17:35'!manoDeObra: costoManoDeObra masMateriales: costoMaterial	^ costoManoDeObra + costoMaterial.! !!AlgoFix3 methodsFor: 'registering' stamp: 'Fer 5/25/2020 18:11'!registrarPintorDePincel: unNombrePintorPincel conValorHora: unValorHora	|pintor pincel|	pincel :=	 Pincel3 inicializarPincelConTiempoPintarUnM2: 2 yUnConsumo: 4.	pintor := Pintor3 inicializarPintorConNombre: unNombrePintorPincel conValorHora: unValorHora yHerramientaDeTrabajo: pincel.	pintores at: unNombrePintorPincel put: pintor.	! !!AlgoFix3 methodsFor: 'creating' stamp: 'Fer 5/24/2020 15:33'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura|	pintura := Pintura3 crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo.	pinturas at: unNombrePintura put: pintura.	^ pintura.	! !Object subclass: #Pintor3	instanceVariableNames: 'nombre valorHora herramienta'	classVariableNames: ''	package: 'TP1-Model3'!!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:43'!calcularMaterial: metros2 conPintura: unaInstanciaPintura 	^ herramienta calcularMaterial: metros2 conPintura: unaInstanciaPintura.! !!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:45'!calcularCostos: metros2 conPintura: unaInstanciaPintura 	|material manoDeObra|	material :=  self calcularMaterial: metros2 conPintura: unaInstanciaPintura .	manoDeObra := self calcularManoDeObra:metros2 conPintura: unaInstanciaPintura .	^ manoDeObra + material.! !!Pintor3 methodsFor: 'calculations' stamp: 'Fer 5/25/2020 18:16'!calcularManoDeObra:metros2 conPintura: unaInstanciaPintura 	|coberturaTiempo|	coberturaTiempo :=  herramienta calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2. 	^ coberturaTiempo * valorHora * metros2 .! !!Pintor3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:31'!initilize	nombre := String.	valorHora:= Integer.	herramienta := ''.	! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombre	nombre:= unNombre.! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!herramienta: unaHerramienta	herramienta:= unaHerramienta.! !!Pintor3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!valorHora: unValorHora 	valorHora:= unValorHora.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor3 class	instanceVariableNames: ''!!Pintor3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:14'!inicializarPintorConNombre:  unNombrePintor conValorHora: unValorHora yHerramientaDeTrabajo: unaHerramienta 	|pintor|	pintor:=	Pintor3 new.	pintor nombre: unNombrePintor. 	pintor valorHora: unValorHora.	pintor herramienta: unaHerramienta.	^pintor.! !Object subclass: #Pintura3	instanceVariableNames: 'nombrePintura precioLitro manosPincel manosRodillo'	classVariableNames: ''	package: 'TP1-Model3'!!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:14'!nombre: unNombrePintura	nombrePintura:= unNombrePintura.	! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosPincel: unasManosPincel	manosPincel:= unasManosPincel.! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!manosRodillo: unasManosRodillo	manosRodillo:= unasManosRodillo.! !!Pintura3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:19'!precioLitro: unPrecioLitro	precioLitro:= 	unPrecioLitro. 	! !!Pintura3 methodsFor: 'accessing' stamp: 'Fer 5/24/2020 16:21'!brindarRendimientoPincel	^ manosPincel.! !!Pintura3 methodsFor: 'accessing' stamp: 'Fer 5/24/2020 16:21'!brindarRendimientoRodillo	^ manosRodillo.! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:19'!brindarCostosRodillo	^ precioLitro * manosRodillo.! !!Pintura3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:18'!brindarCostosPincel	^ precioLitro * manosPincel.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura3 class	instanceVariableNames: ''!!Pintura3 class methodsFor: 'creating' stamp: 'Fer 5/25/2020 18:19'!crearPintura: unNombrePintura conPrecioPorLitro: unPrecioLitro manosPincel: manosPincel manosRodillo: manosRodillo	|pintura |	pintura:= Pintura3 new.	pintura nombre: unNombrePintura.	pintura precioLitro: unPrecioLitro.	pintura manosPincel: manosPincel.	pintura manosRodillo: manosRodillo.	^ pintura.! !Object subclass: #Presupuesto3	instanceVariableNames: 'pintores pintura presupuestos'	classVariableNames: ''	package: 'TP1-Model3'!!Presupuesto3 methodsFor: 'accessing' stamp: 'Fer 5/24/2020 17:12'!valor	^ 	(presupuestos values) min.! !!Presupuesto3 methodsFor: 'accessing' stamp: 'Fer 5/24/2020 17:12'!responsable 	^ presupuestos keyAtValue: ((presupuestos values) min) .! !!Presupuesto3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintura: unaPintura	pintura := unaPintura.! !!Presupuesto3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:22'!pintores: unosPintores	pintores := unosPintores.! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/25/2020 18:11'!elegirPresupuestoMasBarato	^ PresupuestoMasBarato3 dePintor: (self responsable) conValor: (self valor).! !!Presupuesto3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:58'!generarPresupuestos: metros2	presupuestos := pintores collect: [ :pintor |  			pintor calcularCostos: metros2 conPintura: pintura. ].	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto3 class	instanceVariableNames: ''!!Presupuesto3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:22'!crearPresupuestosDe: unosPintores conPintura: unaInstanciaPintura yconMetros: metros2	|presupuestos p|		presupuestos := self new.	presupuestos pintores: unosPintores.	presupuestos pintura: unaInstanciaPintura.	presupuestos generarPresupuestos: metros2.  	p := presupuestos elegirPresupuestoMasBarato.	^ p.! !Object subclass: #PresupuestoMasBarato3	instanceVariableNames: 'valorMinimo pintor'	classVariableNames: ''	package: 'TP1-Model3'!!PresupuestoMasBarato3 methodsFor: 'accesing' stamp: 'Fer 5/24/2020 17:16'!responsable 	^ pintor.! !!PresupuestoMasBarato3 methodsFor: 'accesing' stamp: 'Fer 5/24/2020 17:21'!valor	^ valorMinimo.! !!PresupuestoMasBarato3 methodsFor: 'initialization - private' stamp: 'Fer 5/24/2020 17:59'!guardarMenorPresupuestoDe: unPintor conValor: unValor	pintor := unPintor.	valorMinimo := unValor.! !!PresupuestoMasBarato3 methodsFor: 'initialization' stamp: 'Fer 5/24/2020 17:22'!initialize	pintor := String.	valorMinimo := Integer.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PresupuestoMasBarato3 class	instanceVariableNames: ''!!PresupuestoMasBarato3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/24/2020 17:59'!dePintor: unResponsable conValor: unValor	|presupuestosMasBarato|	presupuestosMasBarato := self new.	presupuestosMasBarato guardarMenorPresupuestoDe: unResponsable conValor: unValor.	^ presupuestosMasBarato.! !Object subclass: #Tecnica3	instanceVariableNames: 'tiempo consumo'	classVariableNames: ''	package: 'TP1-Model3'!!Tecnica3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 14:33'!calcularMaterial: metros2 conPintura: unaPintura	self subclassResponsibility .	! !!Tecnica3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 18:00'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2	self subclassResponsibility .! !!Tecnica3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!tiempo: unTiempo	tiempo:= unTiempo .! !!Tecnica3 methodsFor: 'initialization - private' stamp: 'Fer 5/25/2020 18:26'!consumo: unconsumo	consumo := unconsumo .! !Tecnica3 subclass: #Pincel3	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Pincel3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:53'!calcularMaterial: metros2 conPintura: unaPintura	|rendimiento material|	rendimiento := unaPintura brindarCostosPincel. 	material := metros2 * consumo * rendimiento. 	^ material.! !!Pincel3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:53'!calcularManoDeObraConPintura: unaInstanciaPintura  conMetros: metros2	^ unaInstanciaPintura brindarRendimientoPincel * tiempo * (self determinarDescuento: metros2).! !!Pincel3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:51'!determinarDescuento: metros2 	(metros2 > 40) ifTrue: 		[^ 0.50]		ifFalse:		[^ 1].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pincel3 class	instanceVariableNames: ''!!Pincel3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:26'!inicializarPincelConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|pincel|	pincel := self new.	pincel tiempo: unTiempo.	pincel consumo: unconsumo.	^pincel.! !Tecnica3 subclass: #Rodillo3	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Model3'!!Rodillo3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 16:19'!calcularMaterial: metros2 conPintura: unaPintura	|rendimiento material|	rendimiento := unaPintura brindarCostosRodillo. 	material := metros2 * consumo * rendimiento.	^ material.! !!Rodillo3 methodsFor: 'calculations' stamp: 'Fer 5/24/2020 17:55'!calcularManoDeObraConPintura: unaInstanciaPintura conMetros: metros2	^ unaInstanciaPintura brindarRendimientoRodillo * tiempo.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rodillo3 class	instanceVariableNames: ''!!Rodillo3 class methodsFor: 'as yet unclassified' stamp: 'Fer 5/25/2020 18:26'!inicializarRodilloConTiempoPintarUnM2: unTiempo yUnConsumo: unconsumo	|rodillo|	rodillo := self new.	rodillo tiempo: unTiempo.	rodillo consumo: unconsumo.	^rodillo.! !
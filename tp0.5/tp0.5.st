Object subclass: #Club	instanceVariableNames: 'nombreClub tarifaSocio tarifaSocioVIP recaudacion listadoSocios listadoSociosVIP'	classVariableNames: ''	package: 'tp0.5'!!Club methodsFor: 'initialization' stamp: 'Fernanda 4/29/2020 20:16'!initialize 	nombreClub := String.	tarifaSocio := Integer.	tarifaSocioVIP := Integer.	recaudacion := 0.	listadoSocios := Dictionary new.	listadoSociosVIP := Dictionary new.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 11:21'!nombresSocios	^ (Set withAll: (listadoSocios keys) ).! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/29/2020 20:17'!cantidadDeSocios	^ ((listadoSocios keys) size).! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 11:42'!recaudacionTotal	^ recaudacion.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:21'!establecerTarifaSocio: unaTarifa	tarifaSocio := unaTarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 18:47'!agregarSocioVIP: unSocio 	unSocio  guardarTarifa: tarifaSocioVIP.	listadoSocios at: (unSocio devolverNombre) put: unSocio.	listadoSociosVIP at: (unSocio devolverNombre) put: unSocio.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:16'!cantidadPagadaPor: unNombreDeSocio	^ (listadoSocios at:unNombreDeSocio) devolverCantidadPagada.  ! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 18:47'!agregarSocio: unSocio 	unSocio  guardarTarifa: tarifaSocio.	listadoSocios at: (unSocio devolverNombre) put: unSocio.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/29/2020 13:01'!establecerNombre: aString 	nombreClub := aString.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:21'!establecerTarifaSocioVIP: unaTarifa	tarifaSocioVIP := unaTarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:21'!recibirPagoDe: unNombreDeSocio 	(listadoSocios at: unNombreDeSocio) pagarCuota. 	recaudacion := recaudacion + ((listadoSocios at: unNombreDeSocio) devolverTarifa)! !Object subclass: #Socio	instanceVariableNames: 'nombre cantidadPagada tarifa'	classVariableNames: ''	package: 'tp0.5'!!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:10'!pagarCuota	cantidadPagada := cantidadPagada + tarifa.! !!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 11:36'!devolverCantidadPagada 	^ cantidadPagada.! !!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/29/2020 12:39'!devolverNombre	^ nombre.! !!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/30/2020 18:48'!guardarTarifa: unaTarifa	tarifa := unaTarifa.! !!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 5/2/2020 13:20'!devolverTarifa	^ tarifa.! !!Socio methodsFor: 'as yet unclassified' stamp: 'Fernanda 4/29/2020 12:36'!establecerNombre: unNombre 	nombre := unNombre.! !!Socio methodsFor: 'initialization' stamp: 'Fernanda 4/29/2020 20:06'!initialize	cantidadPagada := 0.! !